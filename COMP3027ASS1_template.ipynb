    "# This function should prepare the data by reading it from a file and converting it into a useful format for training and testing\n",
    "# and implement 90-10 splitting as specified in the project description.\n",
    "import csv\n",
    "import numpy as np\n",
    "\n",
    "def preprocess(dataset, split):\n",
    "    \n",
    "\n",
    "    \n",
    "    #Splits the dataset into a train and a test set\n",
    "    # 90% of the dataset will be in the training set\n",
    "    # 10% of the dataset will be in the test set\n",
    "    train = list()\n",
    "    test = list(dataset)\n",
    "    \n",
    "    train_size = split * len(dataset)\n",
    "    \n",
    "    while len(train) < train_size:\n",
    "        train.append(test.pop(0))\n",
    "\n",
    "    return train, test\n",
    "\n"
   ]
