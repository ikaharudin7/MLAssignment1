    "# This function should calculat prior probabilities and likelihoods (conditional probabilities) from the training data and using\n",
    "# to build a naive Bayes model\n",
    "\n",
    "def train(train_set):\n",
    "    \n",
    "    label_index = 11\n",
    "    \n",
    "    label_list = list()\n",
    "    age_list_under50 = list()\n",
    "    age_list_over50 = list()\n",
    "\n",
    "    hours_list_over50 = list()\n",
    "    hours_list_under50 = list()\n",
    "    \n",
    "    educationNum_list_over50 = list()\n",
    "    educationNum_list_under50 = list()\n",
    "\n",
    "    \n",
    "    scores = list()\n",
    "    \n",
    "    # Contains {label: [Pr(A∩\"<=50K\"),Pr(A∩\">50K\")]}\n",
    "    intersection_dictionary = {}\n",
    "    \n",
    "    # Contains {label: [Conditional probability of \"<=50K\" , Cond prob of \">50K\"]}\n",
    "    conditional_dict = {}\n",
    "    \n",
    "    prior_over50 = 0\n",
    "    prior_under50 = 0\n",
    "    under50_count = 0\n",
    "    over50_count = 0\n",
    "    \n",
    "    # Calculates the prior probability\n",
    "    for entry in train_set:\n",
    "        if entry[label_index].strip() == \"<=50K\":\n",
    "            under50_count += 1\n",
    "        else:\n",
    "            over50_count += 1\n",
    "    \n",
    "    prior_over50 = over50_count/len(train_set)\n",
    "    prior_under50 = under50_count/len(train_set)    \n",
    "        \n",
    "    # Adds each attribute into their own list\n",
    "    for row in train_set:\n",
    "        if row[label_index].strip() == \"<=50K\":\n",
    "            if row[0].strip() != \"?\":\n",
    "                age_list_under50.append(int(row[0]))\n",
    "                \n",
    "            if row[3].strip() != \"?\":\n",
    "                educationNum_list_under50.append(int(row[3]))\n",
    "                \n",
    "            if row[9].strip() != \"?\":\n",
    "                hours_list_under50.append(int(row[9]))\n",
    "        else:\n",
    "            if row[0].strip() != \"?\":\n",
    "                age_list_over50.append(int(row[0]))\n",
    "                \n",
    "            if row[3].strip() != \"?\":\n",
    "                educationNum_list_over50.append(int(row[3]))\n",
    "                \n",
    "            if row[9].strip() != \"?\":\n",
    "                hours_list_over50.append(int(row[9]))\n",
    "\n",
    "    # For each attribute calculate the probability\n",
    "    for i in range(len(train_set[0])-1):\n",
    "        if i == 0:\n",
    "            # Calculate mean and std dev.\n",
    "            mean_under = np.mean(age_list_under50)\n",
    "            std_dev_under = np.std(age_list_under50)\n",
    "            \n",
    "            mean_over = np.mean(age_list_over50)\n",
    "            std_dev_over = np.std(age_list_over50)\n",
    "            \n",
    "            scores.append((mean_under, std_dev_under, mean_over, std_dev_over))\n",
    "        \n",
    "        elif i == 3:\n",
    "            mean_under = np.mean(educationNum_list_under50)\n",
    "            std_dev_under = np.std(educationNum_list_under50)\n",
    "            \n",
    "            mean_over = np.mean(educationNum_list_over50)\n",
    "            std_dev_over = np.std(educationNum_list_over50)\n",
    "            \n",
    "            scores.append((mean_under, std_dev_under, mean_over, std_dev_over))\n",
    "            \n",
    "        elif i == 9:\n",
    "            mean_under = np.mean(hours_list_under50)\n",
    "            std_dev_under = np.std(hours_list_under50)\n",
    "            \n",
    "            mean_over = np.mean(hours_list_over50)\n",
    "            std_dev_over = np.std(hours_list_over50)\n",
    "            \n",
    "            scores.append((mean_under, std_dev_under, mean_over, std_dev_over))\n",
    "            \n",
    "        \n",
    "            \n",
    "        else:\n",
    "            # Calculate the intersection\n",
    "            intersection_dictionary = calculateIntersection(train_set, i)\n",
    "\n",
    "            # Calculates the conditional probabilities\n",
    "            conditional_dict = calculateConditionalProbability(intersection_dictionary, prior_over50, prior_under50)\n",
    "\n",
    "            # Add the dictionary with conditional probabilities to scores\n",
    "            scores.append(conditional_dict)\n",
    "    \n",
    "    # print(scores)\n",
    "    \n",
    "    return scores, [prior_under50, prior_over50]\n"
   ]
