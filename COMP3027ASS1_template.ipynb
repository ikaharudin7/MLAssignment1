    "def load_csv(filename):\n",
    "    # Reads in the dataset\n",
    "    dataset = list()\n",
    "    with open(filename, 'r') as file:\n",
    "        csv_reader = csv.reader(file)\n",
    "        next(csv_reader)\n",
    "        for row in csv_reader:\n",
    "            dataset.append(row)\n",
    "            \n",
    "    return dataset\n",
    "\n",
    "def calculateIntersection(train_set, attribute_index):\n",
    "    '''\n",
    "    Inputs are the training set and the index of the attributee in question.\n",
    "    Calculates the probability of the attribute intersection above 50k and below 50k.\n",
    "    Stores it in a dictionary and returns the dictionary.\n",
    "    '''\n",
    "    label_index = 11\n",
    "    \n",
    "    # Dictionary contains {label:[Pr(A∩\"<=50K\"),Pr(A∩\">50K\")]} \n",
    "    attribute_dictionary = {}\n",
    "    \n",
    "    # Calculate the count for the attribute given the label\n",
    "    for row in train_set:\n",
    "        if row[attribute_index] not in attribute_dictionary:\n",
    "            attribute_dictionary[row[attribute_index]] = [0, 0]\n",
    "        \n",
    "        if row[label_index].strip() == \"<=50K\":\n",
    "            attribute_dictionary[row[attribute_index]][0] += 1\n",
    "        elif row[label_index].strip() == \">50K\":\n",
    "            attribute_dictionary[row[attribute_index]][1] += 1\n",
    "    \n",
    "    \n",
    "    # Perform Laplace Smoothing\n",
    "    for data in attribute_dictionary:\n",
    "        attribute_dictionary[data][0] += 1\n",
    "        attribute_dictionary[data][1] += 1\n",
    "    \n",
    "    \n",
    "    # Calculates the intersection\n",
    "    for data in attribute_dictionary:\n",
    "        attribute_dictionary[data][0] = (attribute_dictionary[data][0])/len(train_set)\n",
    "        attribute_dictionary[data][1] = (attribute_dictionary[data][1])/len(train_set)\n",
    "    \n",
    "    return attribute_dictionary\n",
    "\n",
    "def calculateConditionalProbability(dictionary, prior_over50, prior_under50):\n",
    "    for data in dictionary:\n",
    "        cond_prob_under50 = dictionary[data][0]/prior_under50\n",
    "        cond_prob_over50 = dictionary[data][1]/prior_over50\n",
    "        \n",
    "        dictionary[data] = [cond_prob_under50, cond_prob_over50]\n",
    "\n",
    "    return dictionary\n"
   ]
