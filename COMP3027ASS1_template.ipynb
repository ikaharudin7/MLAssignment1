    "# This function should predict classes for new items in the testing data\n",
    "def predict(test_set, scores, prior_prob, skip=False):\n",
    "    \n",
    "    prediction = list()\n",
    "    under50_score = 0\n",
    "    over50_score = 0\n",
    "    \n",
    "    \n",
    "    for row in test_set:\n",
    "        # print(\"---------\")\n",
    "        # print(row)\n",
    "        under50_score = np.log(prior_prob[0])\n",
    "        over50_score = np.log(prior_prob[1])\n",
    "        \n",
    "        for i in range(len(test_set[0])-1):\n",
    "            if i == 0 and row[0].strip() != \"?\":\n",
    "                mean_under = scores[i][0]\n",
    "                std_under = scores[i][1]\n",
    "                \n",
    "                mean_over = scores[i][2]\n",
    "                std_over = scores[i][3]\n",
    "                \n",
    "                gaussian_under = (1/(std_under*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[0])-mean_under)/std_under)**2)\n",
    "                gaussian_over = (1/(std_over*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[0])-mean_over)/std_over)**2)\n",
    "\n",
    "                under50_score += np.log(gaussian_under)\n",
    "\n",
    "                over50_score += np.log(gaussian_over)\n",
    "                \n",
    "            elif i == 3 and row[0].strip() != \"?\":\n",
    "                mean_under = scores[i][0]\n",
    "                std_under = scores[i][1]\n",
    "                \n",
    "                mean_over = scores[i][2]\n",
    "                std_over = scores[i][3]\n",
    "                \n",
    "                gaussian_under = (1/(std_under*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[3])-mean_under)/std_under)**2)\n",
    "                gaussian_over = (1/(std_over*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[3])-mean_over)/std_over)**2)\n",
    "                                \n",
    "                under50_score += np.log(gaussian_under)\n",
    "                over50_score += np.log(gaussian_over)\n",
    "                \n",
    "            elif i == 9 and row[0].strip() != \"?\":\n",
    "                mean_under = scores[i][0]\n",
    "                std_under = scores[i][1]\n",
    "                \n",
    "                mean_over = scores[i][2]\n",
    "                std_over = scores[i][3]\n",
    "                \n",
    "                gaussian_under = (1/(std_under*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[9])-mean_under)/std_under)**2)\n",
    "                gaussian_over = (1/(std_over*np.sqrt(2*np.pi))) * np.exp(-0.5*((int(row[9])-mean_over)/std_over)**2)\n",
    "                        \n",
    "                under50_score += np.log(gaussian_under)\n",
    "                over50_score += np.log(gaussian_over)\n",
    "            \n",
    "            \n",
    "            elif row[i] in scores[i]:\n",
    "                if skip != True and row[i].strip() != \"?\":\n",
    "                    under50_score += np.log(scores[i][row[i]][0])\n",
    "                    over50_score += np.log(scores[i][row[i]][1])\n",
    "        \n",
    "        # print(under50_score, over50_score)\n",
    "        if(under50_score > over50_score):\n",
    "            prediction.append(\"<=50K\")\n",
    "            # print(\"under\")\n",
    "        else:\n",
